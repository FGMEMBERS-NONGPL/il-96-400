<!-- VS/FPA -->
    <animation>
        <type>select</type>
        <object-name>vs.ind</object-name>
        <object-name>VS.led1</object-name>
        <object-name>VS.led2</object-name>
        <object-name>VS.led3</object-name>
        <object-name>VS.led4</object-name>
