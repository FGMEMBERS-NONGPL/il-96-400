<PropertyList>

<path>APpanel.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>BLimit.knob</object-name>
        <object-name>BLimit.knob.001</object-name>
        <property>instrumentation/afds/inputs/bank-limit-switch</property>
        <factor>-20.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>-0.007</y-m>
            <z-m>-0.006</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

        <animation>
        <type>rotate</type>
        <object-name>IAS.knob</object-name>
        <object-name>IAS.knob.001</object-name>
        <property>instrumentation/afds/inputs/speed-src</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>1</ind><dep>122</dep></entry>
            <entry><ind>2</ind><dep>188</dep></entry>
            <entry><ind>3</ind><dep>239</dep></entry>
        </interpolation>
        <center>
            <x-m>0</x-m>
            <y-m>-0.133</y-m>
            <z-m>-0.018</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

        <animation>
        <type>rotate</type>
        <object-name>ALTmode.knob</object-name>
        <object-name>ALTmode.knob.001</object-name>
        <property>instrumentation/afds/inputs/alt-knob</property>
        <factor>100</factor>
        <center>
            <x-m>0</x-m>
            <y-m>0.135</y-m>
            <z-m>-0.007</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FDtoggle</object-name>
        <property>instrumentation/afds/inputs/FD</property>
        <factor>-30.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>-0.207</y-m>
            <z-m>-0.020</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>THARM.switch</object-name>
        <property>instrumentation/afds/inputs/at-armed</property>
        <factor>-30.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>-0.179</y-m>
            <z-m>0.025</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>THARM1.switch</object-name>
        <property>instrumentation/afds/inputs/at-armed[1]</property>
        <factor>-30.0</factor>
        <center>
            <x-m>0</x-m>
            <y-m>-0.179</y-m>
            <z-m>0.025</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>AP.disengage</object-name>
        <property>instrumentation/afds/inputs/AP-disengage</property>
        <factor>0.011</factor>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
    </animation>

    <animation>
        <type>select</type>
        <object-name>APtoggle</object-name>
        <condition>
            <not>
                <property>autopilot/locks/passive-mode</property>
            </not>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>HDGtoggle</object-name>
        <condition>
            <or>
                <equals>
                    <property>instrumentation/afds/ap-modes/roll-mode</property>
                    <value>HDG SEL</value>
                </equals>
                <equals>
                    <property>instrumentation/afds/ap-modes/roll-mode</property>
                    <value>HDG HOLD</value>
                </equals>
            </or>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>LNAV.toggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/afds/ap-modes/roll-mode</property>
                <value>LNAV</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ALTtoggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/afds/ap-modes/pitch-mode</property>
                <value>ALT</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ATtoggle</object-name>
        <condition>
            <not-equals>
                <property>instrumentation/afds/ap-modes/speed-mode</property>
                <value></value>
            </not-equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>VNAV.toggle</object-name>
        <condition>
            <or>
            <equals>
                <property>instrumentation/afds/ap-modes/pitch-mode</property>
                <value>VNAV ALT</value>
            </equals>
            <equals>
                <property>instrumentation/afds/ap-modes/pitch-mode</property>
                <value>VNAV SPD</value>
            </equals>
            <equals>
                <property>instrumentation/afds/ap-modes/pitch-mode</property>
                <value>VNAV PTH</value>
            </equals>
            </or>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>FLCH.toggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/afds/ap-modes/speed-mode</property>
                <value>FLCH</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>LOC.toggle</object-name>
        <condition>
            <equals>
                <property>instrumentation/afds/ap-modes/roll-mode</property>
                <value>LOC</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>APP.toggle</object-name>
        <condition>
            <or>
                <equals>
                    <property>instrumentation/afds/ap-modes/pitch-mode</property>
                    <value>G/S</value>
                </equals>
                <property>instrumentation/afds/inputs/gs-armed</property>
            </or>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>VS.toggle</object-name>
        <condition>
            <or>
                <equals>
                    <property>instrumentation/afds/ap-modes/pitch-mode</property>
                    <value>V/S</value>
                </equals>
                <equals>
                    <property>instrumentation/afds/ap-modes/pitch-mode</property>
                    <value>FPA</value>
                </equals>
            </or>
        </condition>
    </animation>

<!-- LED displays-->

<!-- IAS/MACH -->
    <animation>
        <type>select</type>
        <object-name>ias.ind</object-name>
        <object-name>IAS.led1</object-name>
        <object-name>IAS.led2</object-name>
        <object-name>IAS.led3</object-name>
        <condition>
            <not>
                <property>instrumentation/afds/inputs/ias-mach-selected</property>
            </not>
        </condition>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>IAS.led1</object-name>
        <property>autopilot/settings/target-speed-kt</property>
        <factor>0.001</factor>
        <step>100</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>IAS.led2</object-name>
        <property>autopilot/settings/target-speed-kt</property>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>IAS.led3</object-name>
        <property>autopilot/settings/target-speed-kt</property>
        <factor>0.1</factor>
        <step>1</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>select</type>
        <object-name>mach.ind</object-name>
        <object-name>MACH.led1</object-name>
        <object-name>MACH.led2</object-name>
        <object-name>MACH.led3</object-name>
        <condition>
            <property>instrumentation/afds/inputs/ias-mach-selected</property>
        </condition>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>MACH.led1</object-name>
        <property>autopilot/settings/target-speed-mach</property>
        <factor>0.1</factor>
        <step>1</step>
        <bias>0.005</bias>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>MACH.led2</object-name>
        <property>autopilot/settings/target-speed-mach</property>
